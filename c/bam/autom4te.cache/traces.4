    AC_ARG_WITH([libcfu-inc],
        AS_HELP_STRING([--with-libcfu-inc=@<:@ARG@:>@],
        [libcfu headers are at this location (ARG=path)]
        ),
        [libcfu_include_dir="$withval"],
        [
